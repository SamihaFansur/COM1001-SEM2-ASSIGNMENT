<!--Form called when a user wants to sign up as a mentor-->
<div class="col">
  <p>Please fill in the information below</p>
  <p>
    <label for="fname" class="options">First name:</label>
    <input type="text" name="fname" id="fname" maxlength="40" size="30" placeholder="Enter first name..."  required value="<%= h @mentors.fname %>"><!--Mandatory field-->
  </p>

  <p>
    <label for="lname" class="options">Last name:</label>
    <input type="text" name="lname" id="lname" maxlength="40" size="30" placeholder="Enter last name..."  required value="<%= h @mentors.lname %>"><!--Mandatory field-->
  </p>

  <p>
    <label for="email" class="options">UoS E-mail:</label>
    <input type="email" name="email" pattern="[A-Za-z0-9._%+-]+@sheffield.ac.uk$" title="Enter your UoS email in the format xyz@sheffield.ac.uk" id="email" placeholder="Enter your UoS email..." maxlength="40" size="30" required value="<%= h @mentors.email %>"><!--Mandatory field-->
  </p>
  
  <p> 
    <label for="phoneNum" class="options">Phone Number:</label>
    <input type="text" name="phoneNum" pattern="^\+(?:[0-9] ?){1,3}\s+[0-9]{6,14}$" title="E.g +12 34567890" id="phoneNum" placeholder="+XX XXXXXXXXXX" maxlength="40" size="30" required value="<%= h @mentors.phoneNum %>"><!--Mandatory field-->
  </p>

